<link rel="stylesheet" href="~/css/createskill.css" />

@model Project

<div id="login-form-wrap">
    <h2>Delete Project</h2>
    <form method="post" id="login-form">
        <p style="padding:20px">
            <input asp-for="Title" type="text" id="username" placeholder="Title" required disabled>
            <i class="validation"><span></span><span></span></i>
        </p>
        <p style="padding:20px">
            <input asp-for="ImageUrl" type="text" id="email" placeholder="Image Url" required disabled><i class="validation"><span></span><span></span></i>
        </p>
        <p style="padding:20px">
            <input asp-for="GithubUrl" type="text" id="email" placeholder="Github Url" required disabled><i class="validation"><span></span><span></span></i>
        </p>
        <p style="padding:20px">
            <input type="submit" id="login" value="Delete Project">
        </p>
    </form>
</div>


